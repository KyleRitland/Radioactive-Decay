    "def relative_and_absolute_error(delta_t_1 , delta_t_2 , delta_t_3 , delta_t_4 , current_quantity_a , current_b_over_a_ratio , gamma):\n",
    "        \n",
    "        \n",
    "    reverse_decay_list1 = radioactive_decay_2_step(t_step , initial_quantity_a , initial_quantity_b)\n",
    "    reverse_N_a_list = reverse_decay_list[0]\n",
    "    reverse_N_b_list = reverse_decay_list[1]\n",
    "    time_list = reverse_decay_list[2]\n",
    "        \n",
    "    analytic_decay_list = analytic_radioactive_decay_2_step( t_step , initial_quantity_a , max(time_list))    \n",
    "    analytic_N_a_list = analytic_decay_list[0]    \n",
    "    analytic_N_b_list = analytic_decay_list[1]    \n",
    "    analytic_time_list = analytic_decay_list[2]    \n",
    "        \n",
    "        \n",
    "    \n",
    "        \n",
    "    abserr_a_list = []\n",
    "    abserr_b_list = []\n",
    "    \n",
    "                 \n",
    "    for counter2 in range(0,len(reverse_N_a_list)):\n",
    "        abserr_a = abs(reverse_N_a_list[counter2]-analytic_N_a_list[counter2])\n",
    "        abserr_b = abs(reverse_N_b_list[counter2]-analytic_N_b_list[counter2])\n",
    "        abserr_a_list.append(abserr_a)\n",
    "        abserr_b_list.append(abserr_b)\n",
    "            \n",
    "    relerr_a_list = [abs(1- reverse_N_a_list[0]/analytic_N_a_list[0])]\n",
    "    relerr_b_list = [0]             \n",
    "        \n",
    "    for counter3 in range(1,len(reverse_N_a_list)):\n",
    "        relerr_a = abs(1- (reverse_N_a_list[counter3])/(analytic_N_a_list[counter3]))\n",
    "        relerr_b = abs(1- (reverse_N_b_list[counter3])/(analytic_N_b_list[counter3]))  \n",
    "        relerr_a_list.append(relerr_a)\n",
    "        relerr_b_list.append(relerr_b)\n",
    "        \n",
    "        \n",
    "    return abserr_a_list,abserr_b_list,relerr_a_list,relerr_b_list,time_list\n",
    "    "
   ]
