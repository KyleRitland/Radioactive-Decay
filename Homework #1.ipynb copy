    "def plot_analytic_and_reverse_decay_parent(t_step , initial_quantity_a , initial_quantity_b):\n",
    "\n",
    "    reverse_decay_list = reverse_radioactive_decay_2_step(t_step , initial_quantity_a , initial_quantity_b)\n",
    "    reverse_N_a_list3 = reverse_decay_list[0]\n",
    "    reverse_N_b_list3 = reverse_decay_list[1]\n",
    "    time_list = reverse_decay_list[2]\n",
    "        \n",
    "    analytic_decay_list = analytic_radioactive_decay_2_step( t_step , initial_quantity_a , initial_quantity_b  , max(time_list))    \n",
    "    analytic_N_a_list = analytic_decay_list[0]  \n",
    "    analytic_N_b_list = analytic_decay_list[1]        \n",
    "    analytic_time_list = analytic_decay_list[2] \n",
    "    \n",
    "    print(len(analytic_N_a_list),len(analytic_N_b_list),len(analytic_time_list))\n",
    "    \n",
    "    fig , ax = plt.subplots()\n",
    "    \n",
    "    ax.plot(time_list,reverse_N_a_list,markersize=2,label = \"Extrapolation Algorithm\")  \n",
    "    ax.plot(analytic_time_list,analytic_N_a_list1,markersize = 2,label = \"Analytic Algorithm\")\n",
    "    plt.xlabel(\"Time\")\n",
    "    plt.ylabel(\"Population\")\n",
    "    plt.title(\"Parent Species Overlay\")\n",
    "    plt.legend(bbox_to_anchor=(.95, .95), loc=1, borderaxespad=0.)"
   ]
