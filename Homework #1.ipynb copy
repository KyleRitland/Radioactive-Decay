  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {
    "collapsed": false,
    "scrolled": true
   },
   "outputs": [],
   "source": [
    "N_a_current = initial_quantity_a\n",
    "    N_b_current = initial_quantity_b\n",
    "    tau_a = 1\n",
    "    tau_b = 3\n",
    "    delta_t = t_step\n",
    "    N_a_list = [N_a_current]\n",
    "    N_b_list = [N_b_current]\n",
    "    one_minus_step_tau_a_frac = 1-delta_t/tau_a\n",
    "    one_minus_step_tau_b_frac = 1-delta_t/tau_b\n",
    "    step_tau_a_frac = delta_t/tau_a\n",
    "    index = 0\n",
    "    \n",
    "    while N_b_current > 0 and index < 10000:\n",
    "        \n",
    "           \n",
    "        \n",
    "        N_a_past = N_a_current/one_minus_step_tau_a_frac\n",
    "        N_b_past = (N_b_current - N_a_past*step_tau_a_frac)/one_minus_step_tau_b_frac\n",
    "        \n",
    "        if N_b_past < 0:\n",
    "            break\n",
    "        else:\n",
    "            index = index + delta_t\n",
    "            N_a_list.append(N_a_past)\n",
    "            N_b_list.append(N_b_past)\n",
    "            N_a_current = N_a_past\n",
    "            N_b_current = N_b_past\n",
    "        \n",
    "    x = frange(0 , (len(N_a_list)-2)*delta_t , delta_t)\n",
    "    \n",
    "    time_list = frange(0,round(index-delta_t,3),delta_t)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {
    "collapsed": false
   },
   "outputs": [],
   "source": [
    "analytic_N_a_list = []\n",
    "    analytic_N_b_list = []\n",
    "    delta_t = t_step\n",
    "    tau_a = 1\n",
    "    tau_b = 1\n",
    "    tau = tau_a/tau_b\n",
    "    time = 0\n",
    "    time_list = frange(0 , time_evolution-delta_t , delta_t)\n",
    "    \n",
    "    N_a_naught = initial_quantity_a/np.exp(-max(time_list)/tau_a)\n",
    "    \n",
    "    N_b_naught = 0\n",
    "    \n",
    "    for counter1 in time_list:\n",
    "        \n",
    "        time_over_tau_a = time/tau_a\n",
    "        time_over_tau_b = time/tau_b\n",
    "\n",
    "        if tau_a != tau_b:\n",
    "            analytic_N_a_f = N_a_naught*(np.exp(-time_over_tau_a))\n",
    "            analytic_N_b_f = N_a_naught*(N_b_naught*np.exp(-time_over_tau_b)+(np.exp(-time_over_tau_a)-np.exp(-time_over_tau_b))/(tau - 1))\n",
    "            analytic_N_a_list.append(analytic_N_a_f)\n",
    "            analytic_N_b_list.append(analytic_N_b_f)                                         \n",
    "            time = time + delta_t\n",
    "        else:\n",
    "            analytic_N_a_f = N_a_naught*np.exp(-time_over_tau_a)\n",
    "            analytic_N_b_f = N_a_naught*(N_b_naught*np.exp(-time_over_tau_a)+(time_over_tau_a)*np.exp(-time_over_tau_a))\n",
    "            analytic_N_a_list.append(analytic_N_a_f)\n",
    "            analytic_N_b_list.append(analytic_N_b_f)                                         \n",
    "            time = time + delta_t"
   ]
