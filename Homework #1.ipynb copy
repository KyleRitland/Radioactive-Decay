    "def reverse_relative_and_absolute_error_different_gamma(delta_t , gamma_1 , gamma_2  , gamma_3 , current_quantity_a , current_b_over_a_ratio ):\n",
    "        \n",
    "        \n",
    "    reverse_decay_list1 = reverse_radioactive_decay_2_step(delta_t , current_quantity_a , current_b_over_a_ratio , gamma_1)\n",
    "    reverse_N_a_list1 = reverse_decay_list1[0]\n",
    "    reverse_N_b_list1 = reverse_decay_list1[1]\n",
    "    time_list1 = reverse_decay_list1[2]\n",
    "    \n",
    "    reverse_decay_list2 = reverse_radioactive_decay_2_step(delta_t , current_quantity_a , current_b_over_a_ratio , gamma_2)\n",
    "    reverse_N_a_list2 = reverse_decay_list2[0]\n",
    "    reverse_N_b_list2 = reverse_decay_list2[1]\n",
    "    time_list2 = reverse_decay_list2[2]\n",
    "    \n",
    "    reverse_decay_list3 = reverse_radioactive_decay_2_step(delta_t , current_quantity_a , current_b_over_a_ratio , gamma_3)\n",
    "    reverse_N_a_list3 = reverse_decay_list3[0]\n",
    "    reverse_N_b_list3 = reverse_decay_list3[1]\n",
    "    time_list3 = reverse_decay_list3[2]\n",
    "        \n",
    "    analytic_decay_list1 = analytic_radioactive_decay_2_step(delta_t , current_quantity_a , current_b_over_a_ratio , time_list1 , gamma_1)    \n",
    "    analytic_N_a_list1 = analytic_decay_list1[0]    \n",
    "    analytic_N_b_list1 = analytic_decay_list1[1]    \n",
    "    analytic_time_list1 = analytic_decay_list1[2]    \n",
    "    \n",
    "    analytic_decay_list2 = analytic_radioactive_decay_2_step(delta_t , current_quantity_a , current_b_over_a_ratio , time_list2 , gamma_2)    \n",
    "    analytic_N_a_list2 = analytic_decay_list2[0]    \n",
    "    analytic_N_b_list2 = analytic_decay_list2[1]    \n",
    "    analytic_time_list2 = analytic_decay_list2[2]    \n",
    "    \n",
    "    analytic_decay_list3 = analytic_radioactive_decay_2_step(delta_t , current_quantity_a , current_b_over_a_ratio , time_list3 , gamma_3)    \n",
    "    analytic_N_a_list3 = analytic_decay_list3[0]    \n",
    "    analytic_N_b_list3 = analytic_decay_list3[1]    \n",
    "    analytic_time_list3 = analytic_decay_list3[2]    \n",
    "        \n",
    "        \n",
    "    print(len(reverse_N_a_list1),len(analytic_N_a_list1),len(time_list1))\n",
    "    print(len(reverse_N_b_list1),len(analytic_N_b_list1),len(time_list1))\n",
    "\n",
    "    \n",
    "    \n",
    "    \n",
    "    abserr_a_list1 = []\n",
    "    abserr_b_list1 = []\n",
    "    \n",
    "                 \n",
    "    for counter1 in range(0,len(time_list1)):\n",
    "        abserr_a = abs(reverse_N_a_list1[counter1]-analytic_N_a_list1[counter1])\n",
    "        abserr_b = abs(reverse_N_b_list1[counter1]-analytic_N_b_list1[counter1])\n",
    "        abserr_a_list1.append(abserr_a)\n",
    "        abserr_b_list1.append(abserr_b)\n",
    "        \n",
    "    abserr_a_list2 = []\n",
    "    abserr_b_list2 = []\n",
    "    \n",
    "                 \n",
    "    for counter2 in range(0,len(time_list2)):\n",
    "        abserr_a = abs(reverse_N_a_list2[counter2]-analytic_N_a_list2[counter2])\n",
    "        abserr_b = abs(reverse_N_b_list2[counter2]-analytic_N_b_list2[counter2])\n",
    "        abserr_a_list2.append(abserr_a)\n",
    "        abserr_b_list2.append(abserr_b)\n",
    "        \n",
    "    abserr_a_list3 = []\n",
    "    abserr_b_list3 = []\n",
    "    \n",
    "                 \n",
    "    for counter3 in range(0,len(time_list3)):\n",
    "        abserr_a = abs(reverse_N_a_list3[counter3]-analytic_N_a_list3[counter3])\n",
    "        abserr_b = abs(reverse_N_b_list3[counter3]-analytic_N_b_list3[counter3])\n",
    "        abserr_a_list3.append(abserr_a)\n",
    "        abserr_b_list3.append(abserr_b)\n",
    "        \n",
    "        \n",
    "        \n",
    "        \n",
    "        \n",
    "            \n",
    "    relerr_a_list1 = [abs(1- reverse_N_a_list1[0]/analytic_N_a_list1[0])]\n",
    "    relerr_b_list1 = [0]             \n",
    "        \n",
    "    for counter4 in range(1,len(time_list1)):\n",
    "        relerr_a = abs(1- (reverse_N_a_list1[counter4])/(analytic_N_a_list1[counter4]))\n",
    "        relerr_b = abs(1- (reverse_N_b_list1[counter4])/(analytic_N_b_list1[counter4]))  \n",
    "        relerr_a_list1.append(relerr_a)\n",
    "        relerr_b_list1.append(relerr_b)\n",
    "        \n",
    "    relerr_a_list2 = [abs(1- reverse_N_a_list2[0]/analytic_N_a_list2[0])]\n",
    "    relerr_b_list2 = [0]             \n",
    "        \n",
    "    for counter5 in range(1,len(time_list2)):\n",
    "        relerr_a = abs(1- (reverse_N_a_list2[counter5])/(analytic_N_a_list2[counter5]))\n",
    "        relerr_b = abs(1- (reverse_N_b_list2[counter5])/(analytic_N_b_list2[counter5]))  \n",
    "        relerr_a_list2.append(relerr_a)\n",
    "        relerr_b_list2.append(relerr_b)\n",
    "        \n",
    "    relerr_a_list3 = [abs(1- reverse_N_a_list3[0]/analytic_N_a_list3[0])]\n",
    "    relerr_b_list3 = [0]             \n",
    "        \n",
    "    for counter6 in range(1,len(time_list3)):\n",
    "        relerr_a = abs(1- (reverse_N_a_list3[counter6])/(analytic_N_a_list3[counter6]))\n",
    "        relerr_b = abs(1- (reverse_N_b_list3[counter6])/(analytic_N_b_list3[counter6]))  \n",
    "        relerr_a_list3.append(relerr_a)\n",
    "        relerr_b_list3.append(relerr_b)\n",
    "    \n",
    "    print(len(time_list1),len(time_list2),len(time_list3))\n",
    "                \n",
    "    return abserr_a_list1,abserr_b_list1,abserr_a_list2,abserr_b_list2,abserr_a_list3,abserr_b_list3,relerr_a_list1,relerr_b_list1,relerr_a_list2,relerr_b_list2,relerr_a_list3,relerr_b_list3,time_list1,time_list2,time_list3\n",
    "    "
   ]
  },
