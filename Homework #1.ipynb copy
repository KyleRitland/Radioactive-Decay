  {
   "cell_type": "code",
   "execution_count": 432,
   "metadata": {
    "collapsed": false
   },
   "outputs": [],
   "source": [
    "def radioactive_decay_2_step(delta_t , current_quantity_a , current_b_over_a_ratio , gamma):\n",
    "    \n",
    "    data_list = reverse_radioactive_decay_2_step(delta_t , current_quantity_a , current_b_over_a_ratio , gamma)\n",
    "    N_a_i = data_list[0][0]\n",
    "    N_b_i = data_list[1][0]\n",
    "    time_list = data_list[2]\n",
    "    N_a_list = [N_a_i]\n",
    "    N_b_list = [N_b_i]\n",
    "    \n",
    "    for counter in time_list:\n",
    "        N_a_f = (1-delta_t)*N_a_i\n",
    "        N_b_f = (1-gamma*delta_t)*N_b_i + N_a_i-N_a_f\n",
    "        N_a_list.append(N_a_f)\n",
    "        N_b_list.append(N_b_f)\n",
    "        N_a_i = N_a_f\n",
    "        N_b_i = N_b_f\n",
    "        \n",
    "        \n",
    "    time_list.append(max(time_list)+delta_t)\n",
    "    \n",
    "    return N_a_list,N_b_list,time_list \n",
    "        "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 433,
   "metadata": {
    "collapsed": false
   },
   "outputs": [],
   "source": [
    "def scaled_radioactive_decay_2_step(delta_t , current_quantity_a , current_b_over_a_ratio , gamma):\n",
    "    \n",
    "    data_list = reverse_radioactive_decay_2_step(delta_t , current_quantity_a , current_b_over_a_ratio , gamma)\n",
    "    N_a_i = data_list[0][0]\n",
    "    N_b_i = data_list[1][0]\n",
    "    time_list = data_list[2]\n",
    "    N_a_list = [N_a_i]\n",
    "    N_b_list = [N_b_i]\n",
    "    \n",
    "    for counter in time_list[1:]:\n",
    "        N_a_f = (1-delta_t)*N_a_i\n",
    "        N_b_f = (1-gamma*delta_t)*N_b_i + N_a_i-N_a_f\n",
    "        N_a_list.append(N_a_f)\n",
    "        N_b_list.append(N_b_f)\n",
    "        N_a_i = N_a_f\n",
    "        N_b_i = N_b_f\n",
    "        \n",
    "\n",
    "    max_N_a = max(N_a_list)\n",
    "    \n",
    "    for counter2 in range(0,len(N_a_list)):\n",
    "        N_a_list[counter2] = N_a_list[counter2]/max_N_a\n",
    "        N_b_list[counter2] = N_b_list[counter2]/max_N_a\n",
    "        \n",
    "    return N_a_list,N_b_list,time_list \n",
    "        "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 434,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "def plot_reversed_radioactive_decay_2_step(delta_t , current_quantity_a , current_b_over_a_ratio , gamma):\n",
    "    \n",
    "    total_data_set = reverse_radioactive_decay_2_step(delta_t , current_quantity_a , current_b_over_a_ratio , gamma)\n",
    "    N_a_list = total_data_set[0]\n",
    "    N_b_list = total_data_set[1]\n",
    "    time_list = total_data_set[2]\n",
    "    \n",
    "    fig , ax = plt.subplots()\n",
    "    \n",
    "    ax.semilogy(time_list,N_a_list,'o',markersize=2,label = \"Parent Species\")\n",
    "    ax.semilogy(time_list,N_b_list,'o',markersize=2,label = \"Daughter Species\")\n",
    "    plt.xlabel(\"Time\")\n",
    "    plt.ylabel(\"Population(Units of N_(t)/N_0)\")\n",
    "    plt.title(\"Numeric Radioactive Decay of 2 species\")\n",
    "    plt.legend(bbox_to_anchor=(.95, .95), loc=1, borderaxespad=0.)\n",
    "    \n",
    "    "
   ]
