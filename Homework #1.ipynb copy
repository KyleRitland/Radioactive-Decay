  {
   "cell_type": "code",
   "execution_count": 58,
   "metadata": {
    "collapsed": false
   },
   "outputs": [],
   "source": [
    "def analytic_radioactive_decay_2_step( delta_t , current_quantity_a , b_over_a_ratio , time_list , gamma):\n",
    "    \n",
    "    \n",
    "    \n",
    "    analytic_N_a_list = []\n",
    "    analytic_N_b_list = []\n",
    "    \n",
    "    max_time = max(time_list)\n",
    " \n",
    "    current_N_b = current_quantity_a*b_over_a_ratio\n",
    "    \n",
    "    N_a_naught = current_quantity_a/np.exp(-max_time)\n",
    "    \n",
    "    if gamma != 1:\n",
    "    \n",
    "        N_b_naught = (current_N_b/N_a_naught - (np.exp(-max_time)-np.exp(-gamma*max_time)))/(gamma-1)\n",
    "    \n",
    "    else:\n",
    "        \n",
    "        N_b_naught = (current_N_b/N_a_naught - max_time*np.exp(-max_time))/np.exp(-max_time)\n",
    "\n",
    "    time = max_time\n",
    "    \n",
    "    for counter in time_list:\n",
    "\n",
    "        gamma_t = gamma*time\n",
    "        \n",
    "        if gamma != 1:\n",
    "            analytic_N_a = N_a_naught*np.exp(-time)\n",
    "            analytic_N_b = N_a_naught*( N_b_naught*np.exp(-gamma_t) + (np.exp(-time) - np.exp(-gamma_t) )/(gamma - 1))\n",
    "            analytic_N_a_list.append(analytic_N_a)\n",
    "            analytic_N_b_list.append(analytic_N_b)                                         \n",
    "            time = time - delta_t\n",
    "        else:\n",
    "            analytic_N_a_f = N_a_naught*np.exp(-time)\n",
    "            analytic_N_b_f = N_a_naught*( N_b_naught*np.exp(-time) + time*np.exp(-time))\n",
    "            analytic_N_a_list.append(analytic_N_a_f)\n",
    "            analytic_N_b_list.append(analytic_N_b_f)                                         \n",
    "            time = time - delta_t\n",
    "            \n",
    "    max_N_a = max(analytic_N_a_list)\n",
    "    \n",
    "    for counter2 in range(0,len(analytic_N_a_list)):\n",
    "        analytic_N_a_list[counter2] = analytic_N_a_list[counter2]/max_N_a\n",
    "        analytic_N_b_list[counter2] = analytic_N_b_list[counter2]/max_N_a\n",
    "        \n",
    "            \n",
    "    return analytic_N_a_list[::-1],analytic_N_b_list[::-1],time_list"
   ]
  },
  {
