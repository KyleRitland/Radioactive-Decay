   "outputs": [],
   "source": [
    "def reverse_radioactive_decay_2_step(delta_t , current_quantity_a , current_b_over_a_ratio , gamma):\n",
    "    \n",
    "    \n",
    "    \n",
    "    N_a = current_quantity_a                             #initializes current population of species p\n",
    "    N_b = current_b_over_a_ratio*current_quantity_a\n",
    "    N_a_list = [N_a]\n",
    "    N_b_list = [N_b]\n",
    "    index = 0\n",
    "    \n",
    "    while 0 < N_b  and index < 10000:\n",
    "        \n",
    "        if (1-gamma*delta_t) == 0 or (1-delta_t) == 0:\n",
    "            break\n",
    "        else:\n",
    "            \n",
    "            N_b = (N_b - N_a*delta_t)/(1-gamma*delta_t)        \n",
    "            N_a = N_a/(1-delta_t)\n",
    "        \n",
    "        if N_b <= 0:\n",
    "            break\n",
    "        elif np.isinf(N_a):\n",
    "            break\n",
    "        elif np.isinf(N_b):\n",
    "            break\n",
    "        elif np.isnan(N_a):\n",
    "            break\n",
    "        elif np.isnan(N_b):\n",
    "            break\n",
    "        else:\n",
    "            index = index + delta_t\n",
    "            N_a_list.append(N_a)\n",
    "            N_b_list.append(N_b)\n",
    "            \n",
    "            \n",
    "    rounder = rounder_size(delta_t)\n",
    "    \n",
    "    time_list = frange(delta_t,delta_t,round((len(N_a_list)+1)*delta_t,rounder))\n",
    "    \n",
    "    max_N_a = max(N_a_list)\n",
    "    \n",
    "    for counter2 in range(0,len(N_a_list)):\n",
    "        N_a_list[counter2] = N_a_list[counter2]/max_N_a\n",
    "        N_b_list[counter2] = N_b_list[counter2]/max_N_a\n",
    "\n",
    "    print(len(N_a_list),len(time_list))\n",
    "        \n",
    "    \n",
    "    return N_a_list[::-1],N_b_list[::-1],time_list"
   ]
  },
