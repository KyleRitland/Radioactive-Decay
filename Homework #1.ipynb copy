   "source": [
    "def plot_analytic_and_reverse_decay_daughter_convergence_test(delta_t_1 , delta_t_2 , delta_t_3 , current_quantity_a , current_b_over_a_ratio , gamma):\n",
    "        \n",
    "    reverse_decay_list = reverse_radioactive_decay_2_step(delta_t_1 , current_quantity_a , current_b_over_a_ratio , gamma)\n",
    "    reverse_N_a_list1 = reverse_decay_list[0]\n",
    "    reverse_N_b_list1 = reverse_decay_list[1]\n",
    "    time_list1 = reverse_decay_list[2]\n",
    "    \n",
    "    reverse_decay_list = reverse_radioactive_decay_2_step(delta_t_2 , current_quantity_a , current_b_over_a_ratio , gamma)\n",
    "    reverse_N_a_list2 = reverse_decay_list[0]\n",
    "    reverse_N_b_list2 = reverse_decay_list[1]\n",
    "    time_list2 = reverse_decay_list[2]\n",
    "    \n",
    "    reverse_decay_list = reverse_radioactive_decay_2_step(delta_t_3 , current_quantity_a , current_b_over_a_ratio , gamma)\n",
    "    reverse_N_a_list3 = reverse_decay_list[0]\n",
    "    reverse_N_b_list3 = reverse_decay_list[1]\n",
    "    time_list3 = reverse_decay_list[2]\n",
    "        \n",
    "    analytic_decay_list = analytic_radioactive_decay_2_step( delta_t_3 , current_quantity_a , current_b_over_a_ratio , time_list3 , gamma)  \n",
    "    analytic_N_a_list = analytic_decay_list[0]  \n",
    "    analytic_N_b_list = analytic_decay_list[1]        \n",
    "    analytic_time_list = analytic_decay_list[2] \n",
    "    \n",
    "    fig , ax = plt.subplots()\n",
    "    \n",
    "    ax.semilogy(time_list1,reverse_N_b_list1,'o-',markersize=2,label = \"Delta_t = 0.5\")  \n",
    "    ax.semilogy(time_list2,reverse_N_b_list2,'o-',markersize=2,label = \"Delta_t = 0.1\")  \n",
    "    ax.semilogy(time_list3,reverse_N_b_list3,'o-',markersize=2,label = \"Delta_t = 0.01\")  \n",
    "    ax.semilogy(analytic_time_list,analytic_N_b_list,'-',markersize = 2,label = \"Analytic Algorithm\")\n",
    "    plt.xlabel(\"t (units of $t/tau_a$)\")\n",
    "    plt.ylabel(\"Population (Units of $N_(t)$\")\n",
    "    plt.title(\"Daughter Species Convergence Test\")\n",
    "    plt.legend(bbox_to_anchor=(.95, .95), loc=1, borderaxespad=0.)"
   ]
  },
