    "def plot_analytic_and_reverse_decay_parent_overlay(delta_t_3 , delta_t_4 , current_quantity_a , current_b_over_a_ratio , gamma):\n",
    "        \n",
    "    \n",
    "    reverse_decay_list = reverse_radioactive_decay_2_step(delta_t_3 , current_quantity_a , current_b_over_a_ratio , gamma)\n",
    "    reverse_N_a_list3 = reverse_decay_list[0]\n",
    "    reverse_N_b_list3 = reverse_decay_list[1]\n",
    "    time_list3 = reverse_decay_list[2]\n",
    "        \n",
    "    analytic_decay_list = analytic_radioactive_decay_2_step( delta_t_4 , current_quantity_a , current_b_over_a_ratio , time_list3 , gamma)    \n",
    "    analytic_N_a_list = analytic_decay_list[0]  \n",
    "    analytic_N_b_list = analytic_decay_list[1]        \n",
    "    analytic_time_list = analytic_decay_list[2] \n",
    "        \n",
    "    fig , ax = plt.subplots()\n",
    "    \n",
    "    \n",
    "    ax.plot(time_list3,reverse_N_a_list3,'bo',markersize = 2,label = \"Extrapolation Algorithm\")\n",
    "    ax.plot(analytic_time_list,analytic_N_a_list,'ro',markersize = 1,label = \"Analytic Algorithm\")\n",
    "    plt.xlabel(\"Time\")\n",
    "    plt.ylabel(\"Population\")\n",
    "    plt.title(\"Parent Species Overlay\")\n",
    "    plt.legend(bbox_to_anchor=(.95, .95), loc=1, borderaxespad=0.)"
   ]
  },
