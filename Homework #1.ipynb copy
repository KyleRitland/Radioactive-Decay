   "source": [
    "def plot_reverse_relative_and_absolute_error(delta_t_1 , delta_t_2 , delta_t_3 , current_quantity_a , current_b_over_a_ratio , gamma):\n",
    "    \n",
    "    error_list = reverse_relative_and_absolute_error(delta_t_1 , delta_t_2 , delta_t_3  , current_quantity_a , current_b_over_a_ratio , gamma)\n",
    "    \n",
    "    f, axarr = plt.subplots(4,1,figsize=(5,15))\n",
    "    axarr[0].plot(error_list[12],error_list[6], 'o',markersize=2 , label = \"delta_t = 0.05\")\n",
    "    axarr[0].plot(error_list[13],error_list[8], 'o',markersize=2, label = \"delta_t = 0.01\")\n",
    "    axarr[0].plot(error_list[14],error_list[10], 'o',markersize=2, label = \"delta_t = 0.001\")\n",
    "    axarr[0].set_title(\"Parent Species Relative Error\")\n",
    "    axarr[0].legend(bbox_to_anchor=(.48, .95), loc=1, borderaxespad=0.)\n",
    "    axarr[1].plot(error_list[12],error_list[0], 'o',markersize=2,label = \"delta_t = 0.05\")\n",
    "    axarr[1].plot(error_list[13],error_list[2], 'o',markersize=2,label = \"delta_t = 0.01\")\n",
    "    axarr[1].plot(error_list[14],error_list[4], 'o',markersize=2,label = \"delta_t = 0.001\")\n",
    "    axarr[1].set_title(\"Parent Species Absolute Error\")\n",
    "    axarr[1].legend(bbox_to_anchor=(.95, .95), loc=1, borderaxespad=0.)\n",
    "    axarr[2].plot(error_list[12],error_list[7], 'o',markersize=2,label = \"delta_t = 0.05\")\n",
    "    axarr[2].plot(error_list[13],error_list[9], 'o',markersize=2,label = \"delta_t = 0.01\")\n",
    "    axarr[2].plot(error_list[14],error_list[11], 'o',markersize=2,label = \"delta_t = 0.001\")\n",
    "    axarr[2].set_title(\"Daughter Species Relative Error\")\n",
    "    axarr[2].legend(bbox_to_anchor=(.95, .95), loc=1, borderaxespad=0.)\n",
    "    axarr[3].plot(error_list[12],error_list[1], 'o',markersize=2,label = \"delta_t = 0.05\")\n",
    "    axarr[3].plot(error_list[13],error_list[3], 'o',markersize=2,label = \"delta_t = 0.01\")\n",
    "    axarr[3].plot(error_list[14],error_list[5], 'o',markersize=2,label = \"delta_t = 0.001\")\n",
    "    axarr[3].set_title(\"Daughter Species Absolute Error\")\n",
    "    axarr[3].legend(bbox_to_anchor=(.95, .95), loc=1, borderaxespad=0.)\n",
    "    f.subplots_adjust(hspace=0.2) \n",
    "    plt.xlabel(\"t (units of $t/tau_a$)\")\n",
    "    plt.ylabel(\"Population (Units of $N_(t)$\")"
   ]
  },
