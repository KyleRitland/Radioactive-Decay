    "def plot_relative_and_absolute_error(t_step , initial_quantity_a , initial_quantity_b):\n",
    "    \n",
    "    error_list = relative_and_absolute_error(t_step , initial_quantity_a , initial_quantity_b)\n",
    "    \n",
    "    f, axarr = plt.subplots(2,2,figsize=(10,10))\n",
    "    axarr[0,1].plot(error_list[4],error_list[2], 'o',markersize=2)\n",
    "    axarr[0,1].set_title(\"Parent Species Relative Error\")\n",
    "    axarr[0,0].plot(error_list[4],error_list[0], 'o',markersize=2)\n",
    "    axarr[0,0].set_title(\"Parent Species Absolute Error\")\n",
    "    axarr[1,1].plot(error_list[4],error_list[3], 'o',markersize=2)\n",
    "    axarr[1,1].set_title(\"Daughter Species Relative Error\")\n",
    "    axarr[1,0].plot(error_list[4],error_list[1], 'o',markersize=2)\n",
    "    axarr[1,0].set_title(\"Daughter Species Absolute Error\")\n",
    "    f.subplots_adjust(hspace=0.2) "
   ]
