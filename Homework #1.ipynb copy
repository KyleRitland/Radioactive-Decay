  {
   "cell_type": "code",
   "execution_count": 60,
   "metadata": {
    "collapsed": false
   },
   "outputs": [],
   "source": [
    "def plot_analytic_radioactive_decay_2_step( delta_t , current_quantity_a , current_b_over_a_ratio , gamma):\n",
    "    \n",
    "    time = reverse_radioactive_decay_2_step(delta_t , current_quantity_a , current_b_over_a_ratio , gamma)\n",
    "    \n",
    "    total_data_set = analytic_radioactive_decay_2_step( delta_t , current_quantity_a , current_b_over_a_ratio , time[2] , gamma)\n",
    "    \n",
    "    fig, ax = plt.subplots()\n",
    "    \n",
    "    ax.semilogy(total_data_set[2],total_data_set[0],'o',markersize=2,label=\"Parent Species\")\n",
    "    ax.semilogy(total_data_set[2],total_data_set[1],'o',markersize=2,label = \"Daughter Species\")\n",
    "    plt.xlabel(\"t (units of $t/tau_a$)\")\n",
    "    plt.ylabel(\"Population (Units of $N_(t)/N_0)$\")\n",
    "    plt.title(\"Analytic Radioactive Decay of 2 species\")\n",
    "    plt.legend(bbox_to_anchor=(.95, .95), loc=1, borderaxespad=0.)\n",
    "\n",
    "    \n",
    "    "
   ]
  },
