    "def plot_reverse_relative_and_absolute_error_different_gamma(delta_t , gamma_1 , gamma_2 , gamma_3 ,  current_quantity_a , current_b_over_a_ratio ):\n",
    "    \n",
    "    error_list = reverse_relative_and_absolute_error_different_gamma(delta_t , gamma_1 , gamma_2  , gamma_3 , current_quantity_a , current_b_over_a_ratio )\n",
    "    \n",
    "    f, axarr = plt.subplots(4,1,figsize=(5,15))\n",
    "    axarr[0].plot(error_list[12],error_list[6], 'o',markersize=5 , label = \"gamma = 0.5\")\n",
