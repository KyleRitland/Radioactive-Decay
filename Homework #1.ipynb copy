    "def reverse_relative_and_absolute_error(delta_t_1 , delta_t_2 , delta_t_3  , current_quantity_a , current_b_over_a_ratio , gamma):\n",
    "        \n",
    "        \n",
    "    reverse_decay_list1 = reverse_radioactive_decay_2_step(delta_t_1 , current_quantity_a , current_b_over_a_ratio , gamma)\n",
    "    reverse_N_a_list1 = reverse_decay_list1[0]\n",
    "    reverse_N_b_list1 = reverse_decay_list1[1]\n",
    "    time_list1 = reverse_decay_list1[2]\n",
    "    \n",
    "    reverse_decay_list2 = reverse_radioactive_decay_2_step(delta_t_2 , current_quantity_a , current_b_over_a_ratio , gamma)\n",
    "    reverse_N_a_list2 = reverse_decay_list2[0]\n",
    "    reverse_N_b_list2 = reverse_decay_list2[1]\n",
    "    time_list2 = reverse_decay_list2[2]\n",
    "    \n",
    "    reverse_decay_list3 = reverse_radioactive_decay_2_step(delta_t_3 , current_quantity_a , current_b_over_a_ratio , gamma)\n",
    "    reverse_N_a_list3 = reverse_decay_list3[0]\n",
    "    reverse_N_b_list3 = reverse_decay_list3[1]\n",
    "    time_list3 = reverse_decay_list3[2]\n",
    "        \n",
    "    analytic_decay_list1 = analytic_radioactive_decay_2_step(delta_t_1 , current_quantity_a , current_b_over_a_ratio , time_list1 , gamma)    \n",
    "    analytic_N_a_list1 = analytic_decay_list1[0]    \n",
    "    analytic_N_b_list1 = analytic_decay_list1[1]    \n",
    "    analytic_time_list1 = analytic_decay_list1[2]    \n",
    "    \n",
    "    analytic_decay_list2 = analytic_radioactive_decay_2_step(delta_t_2 , current_quantity_a , current_b_over_a_ratio , time_list2 , gamma)    \n",
    "    analytic_N_a_list2 = analytic_decay_list2[0]    \n",
    "    analytic_N_b_list2 = analytic_decay_list2[1]    \n",
    "    analytic_time_list2 = analytic_decay_list2[2]    \n",
    "    \n",
    "    analytic_decay_list3 = analytic_radioactive_decay_2_step(delta_t_3 , current_quantity_a , current_b_over_a_ratio , time_list3 , gamma)    \n",
    "    analytic_N_a_list3 = analytic_decay_list3[0]    \n",
    "    analytic_N_b_list3 = analytic_decay_list3[1]    \n",
    "    analytic_time_list3 = analytic_decay_list3[2]    \n",
    "        \n",
    "        \n",
    "    print(min(reverse_N_a_list1),min(analytic_N_a_list1))\n",
    "    print(min(reverse_N_a_list2),min(analytic_N_a_list2))\n",
    "    print(min(reverse_N_a_list3),min(analytic_N_a_list3))\n",
    "  \n",
    "        \n",
    "        \n",
    "    abserr_a_list1 = []\n",
    "    abserr_b_list1 = []\n",
    "    \n",
    "                 \n",
    "    for counter1 in range(0,len(time_list1)):\n",
    "        abserr_a = abs(reverse_N_a_list1[counter1]-analytic_N_a_list1[counter1])\n",
    "        abserr_b = abs(reverse_N_b_list1[counter1]-analytic_N_b_list1[counter1])\n",
    "        abserr_a_list1.append(abserr_a)\n",
