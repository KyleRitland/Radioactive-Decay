   "cell_type": "code",
   "execution_count": 417,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "def has_decimal(number):\n",
    "    \n",
    "    list_check_1 = list(str(number))\n",
    "    \n",
    "    l = len(list_check_1)\n",
    "    \n",
    "    list_check_2 = [ 0 for i in range(l) ]\n",
    "    \n",
    "    for i in range(l):\n",
    "    \n",
    "        if list_check_1[i] == '.':\n",
    "            \n",
    "            list_check_2[i] = 1\n",
    "            \n",
    "        else:\n",
    "            \n",
    "            list_check_2[i] = 0\n",
    "            \n",
    "    return sum(list_check_2)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 418,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "def rounder_size(number):\n",
    "    \n",
    "    if has_decimal(number) == 0:\n",
    "        \n",
    "        rounder = 0\n",
    "        \n",
    "    elif has_decimal(number) != 0:\n",
    "    \n",
    "        number_check = list(str(number))\n",
    "    \n",
    "        index = number_check.index('.')\n",
    "    \n",
    "        rounder = len(number_check[index+1:])\n",
    "    \n",
    "    return rounder"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 419,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "def val_gen(val,rounder):\n",
    "    \n",
    "    if rounder == 0:\n",
    "    \n",
    "        sub_val = val\n",
    "        \n",
    "        val_dec_length = 0\n",
    "        \n",
    "    elif rounder != 0:\n",
    "    \n",
    "        int_size = len(str(val)) - ( rounder + 1 )  \n",
    "        \n",
    "        sub_int = int(''.join( list(str(val))[0:int_size] ))\n",
    "        \n",
    "        if sub_int == 0:\n",
    "            \n",
    "            sub_val = int(''.join( list(str(val))[(int_size+1):] ))\n",
    "            \n",
    "            val_dec_length = len(list(str(val))[(int_size+1):])\n",
    "            \n",
    "        elif sub_int != 0:\n",
    "            \n",
    "            sub_val = int(''.join(( list(str(sub_int)) + list(str(val))[(int_size+1):] )))\n",
    "            \n",
    "            val_dec_length = len(list(str(val))[(int_size+1):])\n",
    "            \n",
    "    return sub_val,val_dec_length\n",
    "            \n",
    "        "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 420,
   "metadata": {
    "collapsed": false
   },
   "outputs": [],
   "source": [
    "def frange(initial,step,final):\n",
    "    \n",
    "    \n",
    "    initial_rounder = rounder_size(initial)\n",
    "    \n",
    "    final_rounder = rounder_size(final)\n",
    "    \n",
    "    step_rounder = rounder_size(step)\n",
    "    \n",
    "    \n",
    "    \n",
    "    \n",
    "    initial_list = val_gen(initial,initial_rounder)\n",
    "    \n",
    "    initial_val = initial_list[0] \n",
    "    \n",
    "    initial_dec_length = initial_list[1]\n",
    "    \n",
    "    step_list = val_gen(step,step_rounder)\n",
    "    \n",
    "    step_val = step_list[0]\n",
    "    \n",
    "    step_dec_length = step_list[1]\n",
    "    \n",
    "    final_list = val_gen(final,final_rounder)\n",
    "    \n",
    "    final_val = final_list[0]\n",
    "    \n",
    "    final_dec_length = final_list[1]\n",
    "    \n",
    "    \n",
    "    \n",
    "    \n",
    "    \n",
    "    \n",
    "    \n",
    "            \n",
    "        \n",
    "    lst_1 = [initial_dec_length , step_dec_length , final_dec_length ]\n",
    "    \n",
    "    lst_2 = [ initial_val , step_val , final_val ]\n",
    "    \n",
    "    max_dec = max(lst_1)\n",
    "    \n",
    "    for i in range(3):\n",
    "        \n",
    "        if lst_1[i] < max_dec:\n",
    "            \n",
    "            lst_2[i] = int(''.join(list(str(lst_2[i])) + [ '0' for i in range(max_dec - lst_1[i]) ]))\n",
    "    \n",
    "    \n",
    "    n_m_l = len( range( min(lst_2[0],lst_2[2]) , max(lst_2[0],lst_2[2]) , lst_2[1] ) )\n",
    "   \n",
    "        \n",
    "        \n",
    "        \n",
    "    \n",
    "            \n",
    "    number_list = [ 0 for i in range( n_m_l ) ]\n",
    "        \n",
    "    index = initial\n",
    "    \n",
    "    true_rounder = max( initial_rounder , step_rounder , final_rounder )\n",
    "        \n",
    "    for i in range( n_m_l ):\n",
    "            \n",
    "        number_list[i] = index\n",
    "            \n",
    "        if initial < final:\n",
    "            \n",
    "            index = round(index + step,true_rounder) \n",
    "                \n",
    "        if initial > final:\n",
    "                \n",
    "            index = round(index - step,true_rounder) \n",
    "            \n",
    "          \n",
    "    \n",
    "    return( number_list ) "
   ]
  },
